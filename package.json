{
  "name": "injectify-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo No test specified!",
    "build": "cd interface/src && npm run build",
    "server": "cross-env NODE_ENV=development nodemon --inspect main",
    "interface": "cd interface/src && npm run dev",
    "dev": "concurrently -n \"webpack,mongo\" -c \"bgGreen.bold,bgMagenta.bold\" \"npm run interface\" \"npm run mongo\"",
    "mongo": "mongod --dbpath=./database --port 19000 --bind_ip 0.0.0.0",
    "pm2": "pm2 start server.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samdenty99/injectify.git"
  },
