    "url": "https://github.com/samdenty99/injectify/issues"
  },
  "homepage": "https://github.com/samdenty99/injectify#readme",
  "dependencies": {
    "atob": "^2.0.3",
    "btoa": "^1.1.2",
    "chalk": "^2.3.0",
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.3",
    "country-emoji": "^1.2.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-rate-limit": "^2.11.0",
    "geoip-lite": "^1.2.1",
    "js-beautify": "^1.7.5",
    "js-obfuscator": "^0.1.2",
